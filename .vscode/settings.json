{
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.js.map": true,
		"**/node_modules": true,
		"**/.history": true,
	},
	"cSpell.words": [
		"astar",
		"chrono",
		"consts",
		"hasher",
		"hashset",
		"homebase",
		"keybinds",
		"Linimik",
		"maplit",
		"pathfinding",
		"pathing",
		"powf",
		"ratelimited",
		"ratelimits",
		"rustc",
		"rustfmt",
		"signum",
		"spritesheet",
		"spritesheets",
		"uvec"
	]
}
